{% extends 'layout/master.twig' %}
{% block head %}
    <style>
        .fError  {
            font-size: 1rem;
            line-height: 1rem;
            margin-top: 0.3rem;
            font-weight: bold;
            text-align: left;
            horiz-align: center;
            background: url(data:image/image/jpeg;base64,) no-repeat transparent;
            background-size:1rem, 1rem;
            padding-left: 1.2rem;
            color: black;
            display: block;
        }
    </style>
{% endblock head %}

{% block content %}
    <div class="container appContent" >
        <div class="row">
            <div class="col-12">
                <div class="title">Validation Rules <small>Using the Framework validation and redirection mechanics.</small></div>
                <hr style="border-bottom: orangered 2px solid">
            </div>
        </div>
        <div class="sectionSeparator"></div>
        <div class="row">
            <div class="col-12">
                <form action="{{ asset(url('Check.validateRaw')) }}" method="post">
                    <div class="form-group">
                        <label>Name</label>
                        <input name="name" type="text" aria-label=""
                               class="form-control"
                               placeholder="Tell us your name"
                               value="{{ oldData.name }}">
                        {% if errors.name %}
                            <div class="fError">
                                {{ errors.name }}
                            </div>
                        {%  endif %}
                    </div>

                    <div class="form-group">
                        <label for="exampleFormControlInput1">Email</label>
                        <input name="email" type="email" aria-label=""
                               class="form-control"
                               placeholder="Tell us your email"
                               value="{{ oldData.email }}">
                        {% if errors.email %}
                            <div class="fError">
                                {{ errors.email }}
                            </div>
                        {%  endif %}
                    </div>
                    <div class="form-group">
                        <label for="exampleFormControlSelect1">Primary Skill</label>
                        <select name="skill" class="form-control" id="exampleFormControlSelect1">
                            <option value="0">Select a Skill</option>
                            {% for skill in skills %}
                                <option value="{{ loop.index }}"
                                        {% if loop.index == oldData.skill %} selected {% endif %}
                                >{{ skill }}</option>
                            {% endfor %}
                        </select>
                        {% if errors.skill %}
                            <div class="fError">
                                {{ errors.skill }}
                            </div>
                        {%  endif %}

                    </div>
                    <div class="form-group">
                        <label for="exampleFormControlTextarea1">About</label>
                        <textarea name="about" aria-label=""
                                  class="form-control"
                                  rows="3">{{ oldData.about }}</textarea>
                        {% if errors.about %}
                            <div class="fError">
                                {{ errors.about }}
                            </div>
                        {%  endif %}

                    </div>
                    <div class="form-check">
                        <input name="terms" aria-label=""
                               class="form-check-input"
                               type="checkbox"
                               value="on"
                                {% if oldData.terms == 'on' %}
                                    checked
                                {% endif %}
                        >
                        <label class="form-check-label" for="defaultCheck1">
                            Agree with the terms
                        </label>
                        {% if errors.terms %}
                            <div class="fError">
                                {{ errors.terms }}
                            </div>
                        {%  endif %}

                    </div>
                    <hr>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
        <div class="sectionSeparator"></div>
    </div>
{% endblock content %}

{% block scripts %}
{% endblock scripts %}

