<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>{{ app_tittle }}</title>
    {{ prodScript(0) | raw  }}
    <link rel="stylesheet" href="{{ asset('./css/main.min.css') }}">
{#    <link rel="stylesheet" href="http://localhost:3000/css/main.min.css">#}
    {% block head %}
    {% endblock head %}
</head>
<body>
<form action="{{ asset('/api/switchLang') }}" method="post" id="formSwitch" style="display: none">
    <input type="hidden" name="language" value="any">
</form>
<div class="cFab">&#9650;</div>
<div class="appHero">
</div>
{{ APP_NAV | raw}}
{% block content %}
{% endblock content %}
<div class="appFooter">
    <span>&#174;&emsp;Webing IT [{{ APP_MODE}}] - {{ now }} </span>
</div>
{{ prodScript(1) | raw  }}
<script src="{{ asset('./js/main.min.js') }}"></script>
{#<script src="http://localhost:3000/js/main.min.js"></script>#}
{% block scripts %}
{% endblock scripts %}
</body>
</html>